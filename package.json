{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev:server": "tsx --watch -r tsconfig-paths/register --inspect=0.0.0.0:9229 ./src/server.ts",
    "typeorm": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:run": "tsx -r tsconfig-paths/register typeorm migration:run -d src/infra/database/typeorm/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitest/coverage-istanbul": "^0.34.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0",
    "vitest-tsconfig-paths": "^3.4.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "log4js": "^6.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17"
  }
}
