{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev:server": "tsx --watch -r tsconfig-paths/register --inspect=0.0.0.0:9229 ./src/server.ts",
    "typeorm": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:generate": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/persistences/typeorm/migrations/update-tables -d ./src/persistences/typeorm/index.ts",
    "typeorm:run": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/persistences/typeorm/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@vitest/coverage-istanbul": "^0.34.1",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vitest": "^0.33.0",
    "vitest-tsconfig-paths": "^3.4.1"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "log4js": "^6.9.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17"
  }
}
