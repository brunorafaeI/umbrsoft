{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "build": "swc src -d build --strip-leading-paths",
    "start": "node -r tsconfig-paths/register build/server.js",
    "dev:server": "tsx --watch -r tsconfig-paths/register --inspect=0.0.0.0:9229 ./src/server.ts",
    "typeorm": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:generate": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/persistences/typeorm/migrations/update-tables -d ./src/persistences/typeorm/index.ts",
    "typeorm:run": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/persistences/typeorm/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.26",
    "@types/cors": "^2.8.14",
    "@types/node": "^22.7.5",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitest/coverage-istanbul": "^2.1.3",
    "eslint": "^9.12.0",
    "git-commit-msg-linter": "^5.0.4",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "json-schema-to-ts": "^3.0.0",
    "lint-staged": "^15.2.10",
    "tsx": "^4.19.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.9.0",
    "vitest": "^2.1.3",
    "vitest-tsconfig-paths": "^3.4.1"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/swagger": "^9.2.0",
    "@scalar/fastify-api-reference": "^1.18.7",
    "dotenv": "^16.3.1",
    "fastify": "^5.0.0",
    "google-auth-library": "^9.14.1",
    "joi-to-json": "^4.3.0",
    "jose": "^5.6.3",
    "log4js": "^6.9.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "ts-json-schema-generator": "^2.3.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20"
  }
}
