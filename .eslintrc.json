{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-useless-constructor": "off",
        "typescript-eslint/prefer-nullish-coalescing": "off"
    },
    "ignorePatterns": [
        "vitest.config.ts", 
        "tsconfig.json"
    ]
}
